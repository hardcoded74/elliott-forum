				<xf:option value="{$version.id}">{$version.version}</xf:option>
			</xf:foreach>
		</xf:selectrow>

		<xf:if is="!$freshInstall">
			<xf:hiddenval name="style_id" value="{$style.style_id}" />
		</xf:if>

		<xf:hiddenval name="step" value="ftp_details" />

		<xf:submitrow icon="submit" />
