		</div>
	</xf:if>

	<div class="block-container">
		<div class="block-body">
			<xf:inforow rowtype="confirm">
				{{ phrase('th_installing_x_confirm_uix', { 'product_name': $product.product_name }) }}
			</xf:inforow>
		</div>

		<xf:selectrow name="version_id" label="{{ phrase('th_version_uix') }}">
